{
	"compilerOptions": {
		"experimentalDecorators": true,
		"lib": ["es2017"],
		"module": "commonjs",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es5"
	}
}